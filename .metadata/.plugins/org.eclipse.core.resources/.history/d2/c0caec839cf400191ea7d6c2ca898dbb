package com.mystudy.ex01_scanner;

import java.util.Collections;
import java.util.HashMap;
import java.util.Map;

public class ScannerExam3_Scissors {
	public static void main(String[] args) {
		//scanner 를 이용한 가위 바위 보 게임
//		123 -1 -2 일시 승리
//		312 1 2일시 승리
//		0일시 무승부
		scissors.print();
	}
	
}
class scissors{
	static final Map<Integer, String> HANDD;
	static{
		HANDD.put(1, "가위");
		HANDD.put(2, "바위");
		HANDD.put(3, "보");
	}
	static void print() {
		System.out.println(HANDD);
		//HANDD.put(3, "d");
	}
	
}
